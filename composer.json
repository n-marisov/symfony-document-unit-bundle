{
  "name": "maris/symfony-document-unit-bundle",
  "description": "Пакет добавляет возможность сохранения и проверки номеров документов.",
  "license": "MIT",
  "version": "0.0.7",
  "type": "symfony-bundle",
  "authors": [
    {
      "name": "Марисов Николай Андреевич",
      "email": "marisov123456789@mail.ru"
    }
  ],
  "require": {
    "php": ">=8.1",
    "ext-bcmath": "*",
    "ext-json": "*",
    "symfony/config": "^6.0",
    "symfony/dependency-injection": "^6.0",
    "doctrine/orm": "^2.0",
    "doctrine/doctrine-bundle": "^2.8",
    "symfony/form": "6.2.*",
    "symfony/validator": "^6.2",
    "twig/extra-bundle": "^2.12|^3.0",
    "twig/twig": "^2.12|^3.0"
  },

  "autoload": {
    "psr-4": {
      "Maris\\Symfony\\DocumentUnit\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Maris\\Symfony\\DocumentUnit\\Test\\": "tests/"
    }
  },
  "require-dev": {
    "phpunit/phpunit": "*",
    "symfony/var-dumper": "*"
  }
}